        "        CREATE TABLE IF NOT EXISTS market_data (\n",
        "            id INTEGER PRIMARY KEY AUTOINCREMENT,\n",
        "            crop_name TEXT,\n",
        "            market_price REAL,\n",
        "            demand_score REAL,\n",
        "            weather_impact REAL\n",
        "        )\n",
        "    \"\"\")\n",
        "    conn.commit()\n",
        "    conn.close()\n",
        "\n",
        "# Insert Data into Database\n",
        "def insert_market_data():\n",
